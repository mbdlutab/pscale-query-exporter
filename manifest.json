{
  "name": "Planetscale Query Exporter",
  "description": "Can export the results from SELECT queries to CSV or JSON format.",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "matches": ["*://*.planetscale.com/*"],
      "js": ["content.js"]
    }
  ]
}
